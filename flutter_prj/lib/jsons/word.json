{
    "name @primary": "",
    "voiceUs": "",
    "voiceUk": "",
    "morph": [""],
    "tag": [""],
    "etyma": [""],
    "origin": "",
    "shorthand": "",
    "synonym": [""], //  # 近义词 [word1,word2,...]
    "antonym": [""],// 反义词 [word1,word2,...]
    "_image": "", 
    "_vedio": "",
    "_paraphraseSet": ["$paraphrase"], // 释义
    "_sentencePatternSet": ["$sentence_pattern"], // 常用句型
    "_grammarSet": ["$grammar"], //语法
    "_distinguishSet": ["$distinguish"], // 词义辨析
    "__http__": {
        "url": "/dictionary/word/",
        "http_package": "package:flutter_prj/common/http.dart",
        "methods": [
            {
                "name": "create"
            },
            {
                "name": "update",
                "url": "$name/"
            },
            {
                "name": "retrieve",
                "url": "$name/"
            },
            {
                "name": "delete",
                "url": "$name/"
            },
            {
                "name": "save"
            }
        ]
    }
}


{ // 分页查询 单词
    "__name__": "word_pagination",
    "count": 0,
    "next": "",
    "previous": "",
    "results": ["$word"],
    "__filter__": {
        "__serializer__": "$word",
        "name": ["exact","icontains"],
        "tag": ["icontains"],
        "etyma": ["icontains"]
    },
    "__http__": {
        "url": "/dictionary/word/",
        "http_package": "package:flutter_prj/common/http.dart",
        "methods": [
            {
                "name": "retrieve",
                "filter": true
            }
        ]
    }
}


{ // 常用句型
    "__name__": "sentence_pattern",
    "id @primary": "num=null",
    "content": "",
    "wordForeign @foreign": "$word",
    "_paraphraseSet": ["$paraphrase"]
}

{ // 释义
    "__name__": "paraphrase",
    "id @primary": "num=null",
    "interpret": "",
    "partOfSpeech": "",
    "wordForeign @foreign": "$word",
    "sentencePatternForeign @foreign": "$sentence_pattern",
    "_sentenceSet": ["$sentence"]
}

{
    "__name__": "word_tag",
    "name @primary": "",
    "__http__": {
        "url": "/dictionary/word_tag/",
        "http_package": "package:flutter_prj/common/http.dart",
        "methods": [
            {
                "name": "create"
            },
            {
                "name": "list"
            },
            {
                "name": "delete",
                "url": "$name/"
            }
        ]
    }
}


{  // 词根词缀表
    "__name__": "etyma",
    "name @primary": "",
    "interpretation": "",
    "type": "num=null",
    "_image": "", 
    "_vedio": "",
    "__http__": {
        "url": "/dictionary/etyma/",
        "http_package": "package:flutter_prj/common/http.dart",
        "methods": [
            {
                "name": "create"
            },
            {
                "name": "update",
                "url": "$name/"
            },
            {
                "name": "retrieve",
                "url": "$name/"
            },
            {
                "name": "delete",
                "url": "$name/"
            }
        ]
    }
}

{ // 分页查询 词根词缀
    "__name__": "etyma_pagination",
    "count": 0,
    "next": "",
    "previous": "",
    "results": ["$etyma"],
    "__filter__": {
        "__serializer__": "$etyma",
        "name": ["exact","icontains"],
        "type": ["exact"]
    },
    "__http__": {
        "url": "/dictionary/etyma/",
        "http_package": "package:flutter_prj/common/http.dart",
        "methods": [
            {
                "name": "retrieve",
                "filter": true
            }
        ]
    }
}
