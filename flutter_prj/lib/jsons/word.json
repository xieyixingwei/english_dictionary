    "__filter__": {
        "__serializer__": "$word",
        "w_name": ["exact","icontains"],
        "w_tags": ["icontains"],
        "w_etyma": ["icontains"]
        //"w_partofspeech": ["icontains"]
    },
    "__http__": {
        "url": "/dictionary/word",
