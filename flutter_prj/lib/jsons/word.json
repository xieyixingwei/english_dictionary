{
    "w_name @primary": "",
    "w_voice_us": "",
    "w_voice_uk": "",
    "w_morph": [""],
    "w_tags": [""],
    "w_etyma": [""],
    "w_origin": "",
    "w_shorthand": "",
    "w_partofspeech": ["$part_of_speech"], // 词性
    "w_sentence_pattern": ["$sentence_pattern"], // 常用句型
    "w_word_collocation": ["$word_collocation"], // 词汇搭配
    "w_synonym": [""], //  # 近义词 [word1,word2,...]
    "w_antonym": [""],// 反义词 [word1,word2,...]
    "distinguish": ["$distinguish"], //models.JSONField()     # 词义辨析id [id1,id2,...]
    "word_grammar": ["$grammar"], //models.JSONField() # 语法id [id1,id2,...]
	"__http__": {
        "url": "/dictionary/word/",
        "http_package": "package:flutter_prj/common/http.dart",
        "methods": [
            {
                "name": "create",
                "url": "create/"
            },
            {
                "name": "update",
                "requst": "put",
                "url": "update/$w_name/"
            },
            {
                "name": "retrieve",
                "requst": "get",
                "url": "$w_name/"
            },
            {
                "name": "delete",
                "url": "delete/$w_name/"
            },
			{
                "name": "save"
            }
        ]
    }
}


