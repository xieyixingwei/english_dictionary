{
    "name @pk": "",
    "voiceUs": "",
    "voiceUk": "",
    "_audioUsMan": "String=null",
    "_audioUsWoman": "String=null",
    "_audioUkMan": "String=null",
    "_audioUkWoman": "String=null",
    "morph": [""],
    "tag": [""],
    "etyma": [""],
    "origin": "",
    "shorthand": "",
    "synonym @fk_mm": ["$word"], //  # 近义词 [word1,word2,...]
    "antonym @fk_mm": ["$word"],// 反义词 [word1,word2,...]
    "image": "$SingleFile image", 
    "vedio": "$SingleFile video",
    "_paraphraseSet": ["$paraphrase"], // 释义
    "_sentencePatternSet": ["$sentence_pattern"], // 常用句型
    "_grammarSet": ["$grammar"], //语法
    "_distinguishSet": ["$distinguish"], // 词义辨析
    "__http__": {
        "url": "/api/dictionary/word/",
        "methods": [
            {
                "name": "create"
            },
            {
                "name": "update",
                "url": "$name/"
            },
            {
                "name": "retrieve",
                "url": "$name/"
            },
            {
                "name": "delete",
                "url": "$name/"
            },
            {
                "name": "save"
            }
        ]
    }
}


{ // 分页查询 单词
    "__name__": "word_pagination",
    "count": 0,
    "next": "",
    "previous": "",
    "results": ["$word"],
    "__filter__": {
        "__serializer__": "$word",
        "name": ["exact","icontains"],
        "tag": ["icontains"],
        "etyma": ["icontains"]
    },
    "__http__": {
        "url": "/api/dictionary/word/",
        "methods": [
            {
                "name": "retrieve",
                "filter": true
            }
        ]
    }
}


