{
    "id @pk": "num=null", //models.AutoField(primary_key=True)
    "en": "", // 英文
    "_enVoice": "",
    "cn": "", // 中文
    "_cnVoice": "",
    "type": 0, //  models.IntegerField(default=0)  # sentence|phrase
    "tag": [""], // models.JSONField(null=True)    # 标记 [日常用语,商务用语]
    "tense": "", //  models.JSONField(null=True)   # 时态 [一般过去式,被动语态]
    "pattern": [""], // models.JSONField(null=True)    # 句型 [复合句,问候语,从句,陈述句]
    "synonym": [0], //models.JSONField(null=True) # 同义句 [Sentence.id,Sentence.id,...]
    "antonym": [0], //models.JSONField(null=True) # 反义句 [Sentence.id,Sentence.id,...]
    "paraphraseForeign @fk_mo": "$paraphrase",
    "dialogForeign @fk_mo": "$dialog",
    "_grammarSet @fk_slave": ["$grammar"], //models.JSONField() # 语法 [id1,id2,id3]
    "__offstage": true,
    "___synonymObjes": ["$sentence"],
    "__http__": {
        "url": "/api/dictionary/sentence/",
        "methods": [
            {
                "name": "create"
            },
            {
                "name": "update",
                "url": "$id/"
            },
            {
                "name": "retrieve",
                "url": "$id/"
            },
            {
                "name": "delete",
                "url": "$id/"
            },
            {
                "name": "save"
            }
        ]
    }
}

{ // 分页查询 句子
    "__name__": "sentence_pagination",
    "count": 0,
    "next": "",
    "previous": "",
    "results": ["$sentence"],
    "__filter__": {
        "__serializer__": "$sentence",
        "en": ["icontains"],
        "cn": ["icontains"],
        "type": ["exact"],
        "tag": ["icontains"],
        "tense": ["exact"],
        "pattern": ["icontains"]
    },
    "__http__": {
        "url": "/api/dictionary/sentence/",
        "methods": [
            {
                "name": "retrieve",
                "filter": true
            }
        ]
    }
}

